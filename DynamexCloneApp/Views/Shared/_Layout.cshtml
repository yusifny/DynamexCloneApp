<!DOCTYPE html>
<html lang="az">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/imgs/main/favicon.png" type="image/x-icon">
    <!-- BOOTSTRAP STYLESHEET CDN STARTS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <!-- BOOTSTRAP STYLESHEET CDN ENDS -->
    <!-- LOCAL STYLESHEETS START -->
    <link href="@Url.Content("~/css/shared.css")" rel="stylesheet" type="text/css"/>
    @RenderSection("styles", false)
    @RenderSection("scripts", false)
    <!-- LOCAL STYLESHEETS END -->
    <title>@ViewBag.Title</title>
</head>
<noscript>
    <style type="text/css">
        body{ display:none; }
    </style>
</noscript>
<body>
<div id="preloader" class="preloader">
    <div class="preloader-inner">
        <div class="preloader-icon">
            <img src="imgs/main/preloader.gif" alt="preloader" width="100%" height="100%" draggable="false">
        </div>
    </div>
</div>
<!-- HEADER STARTS -->
<header>
    <div class="top-header py-2">
        <div class="container">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="row align-items-center">
                    <div class="nav justify-content-between">
                        <div class="top-header-left-side d-flex gap-5 align-items-center">
                            <a href="tel:*7171" class="d-flex gap-2">
                                <span class="iconify"
                                      data-icon="healthicons:phone" data-height="16"
                                      style="color: #7b7b93;">
                                </span>*7171
                            </a>
                            <a asp-controller="ProhibitedItems">Daşınması qadağan olan məhsullar</a>
                            <a asp-controller="Faq">Tez-tez verilən suallar</a>
                        </div>
                        <div class="top-header-right-side d-flex gap-4 align-items-center">
                            <a href="#" class="d-flex align-items-center gap-1 lang-all" id="lang-all">
                                <span class="lang" id="lang" style="position: relative;">AZ</span>
                                <div class="lang-drop-down" id="lang-drop-down">
                                    <span class="aze selected" id="aze">AZ</span>
                                    <span class="eng" id="eng">EN</span>
                                    <span class="rus" id="rus">RU</span>
                                </div>
                                <span class="iconify chevron" id="chevron" data-icon="ci:chevron-big-down"
                                      style="color: black;">
                                </span>
                            </a>
                            <a asp-controller="Login" class="sign-in">Daxil ol</a>
                            <a asp-controller="Register" class="sign-up">Qeydiyyat</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="bottom-header py-4">
        <div class="container py-1">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="row align-items-center">
                    <div class="nav justify-content-between align-items-center">
                        <div class="bottom-header-left-side pt-1">
                            <a asp-controller="Home" class="logo">
                                <img src="imgs/main/logo.svg" alt="Dynamex Logo" height="53" class="logo">
                            </a>
                        </div>
                        <div class="bottom-header-menu">
                            <nav>
                                <ul class="nav d-flex gap-5 align-items-center">
                                    <li>
                                        <a asp-controller="About" asp-action="Index">Haqqımızda</a>
                                    </li>
                                    <li>
                                        <a asp-controller="Services">Xidmətlər</a>
                                    </li>
                                    <li>
                                        <a asp-controller="Rates">Tariflər</a>
                                    </li>
                                    <li>
                                        <a asp-controller="Stores">Mağazalar</a>
                                    </li>
                                    <li>
                                        <a asp-controller="News">Yeniliklər</a>
                                    </li>
                                    <li>
                                        <a asp-controller="Contact">Əlaqə</a>
                                    </li>
                                    <a href="#" class="cart">
                                        <span class="iconify" data-icon="ic:outline-shopping-cart"
                                              style="position: relative;">
                                        </span>
                                        <span class="item-count">0</span>
                                    </a>
                                </ul>
                            </nav>
                        </div>
                        <div class="bottom-header-right-side">
                            <a asp-controller="Login" class="sign-in">Daxil ol</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<!-- HEADER ENDS -->

@RenderBody()

